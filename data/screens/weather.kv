#:import math math
#:import webbrowser webbrowser
ShowcaseScreen:
  name: 'weather'
  fullscreen: True

  BoxLayout:
    orientation: 'vertical'

    # today
    FloatLayout:

      Button:
        pos_hint: {'center': (app.sunhour_x[0], app.sunhour_y[0])}
        size_hint: None, None
        size: app.iconsize, app.iconsize * 3
        background_color: 0,0,0,0
        on_press: app.weather_details(0)

        Label:
          text: app.weather_temp[0]
          markup: True
          center_x: self.parent.center_x
          center_y: self.parent.center_y + app.iconsize
          size_hint: None, None
          size: app.iconsize, app.iconsize / 2
        Image:
          source: app.weather_icons[0]
          center_x: self.parent.center_x
          center_y: self.parent.center_y
          size_hint: None, None
          size: app.iconsize, app.iconsize
        Label:
          text: app.weather_prob[0]
          center_x: self.parent.center_x
          center_y: self.parent.center_y - app.iconsize
          size_hint: None, None
